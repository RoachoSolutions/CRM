@model List<RoachoSLN.Entidades.CRM.ProspectoHistorico>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Historial de Movimientos</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                    </div>
                    <div class="ibox-content">       
                        <table class="table">
                            <thead>
                                <tr>                                    
                                    <th>Usuario</th>
                                    <th>Movimiento</th>
                                    <th>Fecha</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (RoachoSLN.Entidades.CRM.ProspectoHistorico _historial in Model)
                                {
                                    <tr>
                                        <td>
                                            @_historial.usuario
                                        </td>
                                        <td>
                                            @_historial.movimiento
                                        </td>
                                        <td>
                                            @_historial.fecha
                                        </td>
                                    </tr>
                                }       
                            </tbody>
                        </table>    
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>